  "components": {
    "schemas": {
      "ItemDTO": {
        "required": [
          "itemName",
          "itemPrice"
        ],
        "type": "object",
        "properties": {
          "itemName": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "itemPrice": {
            "type": "number",
            "format": "double"
          },
          "itemDescription": {
            "maxLength": 300,
            "type": "string",
            "nullable": true
          },
          "imageUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MenuDTO": {
        "type": "object",
        "properties": {
          "menuDTO": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrderDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "OrderDTO": {
        "required": [
          "itemName",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "itemName": {
            "minLength": 1,
            "type": "string"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "RestaurantDTO": {
        "required": [
          "address",
          "restaurantName",
          "type"
        ],
        "type": "object",
        "properties": {
          "restaurantName": {
            "maxLength": 255,
            "minLength": 1,
            "type": "string"
          },
          "address": {
            "maxLength": 255,
            "minLength": 1,
            "type": "string"
          },
          "type": {
            "minLength": 1,
            "type": "string"
          },
          "parkingLot": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "UserDTO": {
        "required": [
          "password",
          "userEmail"
        ],
        "type": "object",
        "properties": {
          "userEmail": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 30,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
